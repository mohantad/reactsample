{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
        "no-var-requires": false,
        "no-console": false,
        "max-classes-per-file": [false, 1],
        "jsx-no-lambda": false,
        "ordered-imports": [false],
        "object-literal-sort-keys": [false]
    }
}
